#include<iostream>
#include<vector>
#define max 10001
using namespace std;
vector <int> c;
void nhapMang(int a[], int n){
	for(int i=0;i<n;i++)
		cin>>a[i];
}
bool checkDaXet(int x){
	for(int i=0;i<c.size();i++)
		if(x==c[i])
			return true;
	return false;
}
void union2SortedArray(int a[], int n, int b[], int m){
	int maxPhanTu=b[m-1];
	if(a[n-1]>b[m-1])
		maxPhanTu=a[n-1];
	for(int i=0;i<n;i++)
		if(checkDaXet(a[i])==false)
			c.push_back(a[i]);
	for(int i=0;i<m;i++)
		if(checkDaXet(b[i])==false)
			c.push_back(b[i]);
	for(int i=0;i<c.size();i++)
		cout<<c[i]<<" ";
}
int main(int argc, char *argv[])
{
	int n,m, a[max], b[max];
	cin>>n>>m;
	nhapMang(a,n);
	nhapMang(b,m);
	union2SortedArray(a,n,b,m);
	return 0;
}
