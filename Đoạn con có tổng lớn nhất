#include<iostream>
using namespace std;
#include<vector>
int n;
long long sum=0, answer=0;
int main(int argc, char *argv[])
{
	//answer max=2*10^14>INT MAX, use long long for answer
	cin>>n;
	vector<int>a(n);
	for(int i=0;i<n;i++)
		cin>>a[i];
	if(*max_element(a.begin(),a.end())<=0)
		*max_element(a.begin(),a.end());
	else
		for(int i=0;i<n;i++){
			if(sum<0){
				sum=0;
			}
			sum+=a[i];
			answer=max(answer,sum);
		}
	cout<<answer;
	return 0;
}
//neu day toan so<=0 thi day con la 1 so lon nhat
/*
in:
7
3 -2 5 3 -5 2 2
out: 9
*/
