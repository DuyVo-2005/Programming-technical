//chia du cho 9, dat du la phan tu dau day, cac so con lai quy thanh it so 9 nhat
//in: 10
//out: 19
//tham khao Tran Canh Nam
#include<iostream>
using namespace std;
int main(){
	int n;
	cin>>n;
	if(n/9!=0)
		cout<<n%9;
	for(int i=1;i<=n/9;i++)
		cout<<9;
	return 0;
}
