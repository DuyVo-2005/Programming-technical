#include <iostream>
using namespace std;
#include <vector>
#include <string.h>
char s1[100], s2[100];
void xuat(vector<int> a)
{
	for (int i = a.size() - 1; i >= 0; i--)
		cout << a[i];
}
void cong(vector<int> a, vector<int> b)
{
	vector<int> kq;
	int max_len = strlen(s1);
	if (a.size() > b.size())
	{
		for (int i = b.size(); i < a.size(); i++)
			b.push_back(0);
	}
	else
	{
		max_len = strlen(s2);
		for (int i = a.size(); i < b.size(); i++)
			a.push_back(0);
	}
	int memory = 0;
	for (int i = 0; i < max_len; i++)
	{
		int tmp = a[i] + b[i] + memory;
		if (tmp >= 10)
		{
			memory = tmp / 10;
			kq.push_back((tmp % 10));
		}
		else
		{
			kq.push_back(tmp);
			memory = 0;
		}
	}
	if (memory != 0)
	{
		kq.push_back(memory);
	}
	for (int i = kq.size() - 1; i >= 0; i--)
		cout << kq[i];
}
int main(int argc, char *argv[])
{
	cin.getline(s1, 100);
	cin.getline(s2, 100);
	vector<int> a;
	vector<int> b;
	for (int i = strlen(s1) - 1; i >= 0; i--)
		a.push_back(s1[i] - '0');
	for (int i = strlen(s2) - 1; i >= 0; i--)
		b.push_back(s2[i] - '0');
	cong(a, b);
}
